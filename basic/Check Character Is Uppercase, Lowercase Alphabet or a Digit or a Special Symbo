//Check Character Is Uppercase, Lowercase Alphabet or a Digit or a Special Symbole

#include<stdio.h>
main(){
    char a,b;
    printf ("enter the value");
    scanf ("%c",&a);
    b=a;
    printf ("%d\n",b);
    if (b<=90&&b>=65){
        printf ("Upper case");
    }
    else if (b<=122&&b>=97){
        printf ("Lower case");
    }
    else if (b<=57&&b>=48){
        printf ("numbers");
    }
    else if (b<=32&&b>=0){
        printf ("Digit ");
    }
    else{
        printf ("Special Symbol");
    }
}



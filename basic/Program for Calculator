//23)	Program for Calculator

#include <stdio.h>
main(){
    int a,b,c,d;
    printf ("enter the value");
    scanf ("%d",&a);
    printf ("enter the value");
    scanf ("%d",&b);
    printf ("enter the value");
    scanf ("%d",&c);
    switch (a){
        case 1:
        d=b+c;
        printf ("%d",d);
        break;
        case 2:
        d=b-c;
        printf ("%d",d);
        break;
        case 3:
        d=b*c;
        printf ("%d",d);
        break;
        case 4:
        d=b/c;
        printf ("%d",d);
        break;
        case 5:
        d=b%c;
        printf ("%d",d);
        break;
    }
}
